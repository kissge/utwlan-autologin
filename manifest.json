{
    "name": "UT WLAN Auto Login",
    "version": "0.0.1",
    "author": "KISUGE, Yuto",
    "manifest_version": 2,
    "description": "東京大学の学内無線LANサービス（一部有線）の認証を自動で行う",
    "permissions": ["https://scw.ecc.u-tokyo.ac.jp/cgi-bin/", "storage"],
    "content_scripts": [
        {
            "matches": ["https://scw.ecc.u-tokyo.ac.jp/cgi-bin/*"],
            "js": ["jquery-2.1.1.min.js", "main.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": ["options.html"],
    "options_page": "options.html"
}
